public class Sort {
	public int compares;
	
	public void bruteSort(int[] array){
		for(int j=0; j<array.length;j++)
		 {
		     for (int i=j+1 ; i<array.length; i++)
		     {
		    	 if(array[i] < array[j])
		         {
		             int temp = array[j];
		             array[j] = array[i]; 
		             array[i] = temp;
		             compares++;
		         }
		     }

		 }

        //System.out.print(array[i] + " ");
	}
	
	
}
