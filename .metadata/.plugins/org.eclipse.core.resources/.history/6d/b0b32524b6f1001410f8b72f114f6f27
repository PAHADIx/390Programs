
public class Queue {
	
	private int front, rear;
	private int[] queue = new int[20];
	
	public Queue()
	{
		front = 0;
		rear = -1;
	}
	
	public void push(int num)
	{
		rear++;
		queue[rear] = num;
			
	}
	
	public int pop(){
		if(isEmpty()){
			System.out.println("Empty Queue");
		} else {
			int temp = queue[front];
			front++;
			return temp;	
		}
	}
	
	public boolean isEmpty(){
		if(front >= rear){
			return true;
		}
		return false;
	}
	
	

}
