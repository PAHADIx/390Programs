#include <stdio.h>
void rowPrinter(int[]);

void second(){

	printf("\n\nQuestion 2 :\n");
	//Question 2 : Make a 2D Array of 10x10
	int array2[10][10];

	//2A/B Using pointer fill from 0 to 99
	int* ptr2 = &array2[0][0];
	int i;
	for(i = 0; i < 100; i++){
		*ptr2 = i;
		ptr2++;
	}

	//2C Write a function to print a row
	//rowPrinter() below

	//2D Print all using rowPrinter
	for(i = 0; i < 10; i++){
		rowPrinter(array2[i]);
	}



}

void rowPrinter(int[] a){
	int* temp = a[0];
	int i;
	for(i = 0; i < sizeof(a); i++){
		printf("%d",*temp);
		temp++;
	}
}
