
public class Stack {
	private int top;
	private int[] stack = new int[20];
	
	public Stack(){
		top = -1;
	}
	
	public void push(int num){
		top++;
		stack[top] = num;
	}
	
	public int pop(){
		if(isEmpty()){
			System.out.println("Stack Empty");
		} else {
			int temp = stack[top];
			top--;
			return temp;	
		}
	}
	
	public boolean isEmpty(){
		if(top == -1){
			return true;
		}
		return false;
	}
	

}
