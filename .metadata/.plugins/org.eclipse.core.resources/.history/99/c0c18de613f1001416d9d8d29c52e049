public class Sort {
	public int compares;
	
	public void bruteSort(int[] array){
		for(int j=0; j<array.length;j++)
		 {
		     for (int i=j+1 ; i<array.length; i++)
		     {
		    	 if(names[i].trim().compareTo(names[j].trim())<0)
		         {
		             String temp = names[j];
		             names[j] = names[i]; 
		             names[i] = temp;
		         }
		     }

		 }
	}
	
	
}
