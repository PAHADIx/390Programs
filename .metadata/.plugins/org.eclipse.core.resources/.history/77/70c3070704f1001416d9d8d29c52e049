public class Tree {
	public TreeNode root;

	public Tree(){
		root = TreeNode();
	}
	
	public Tree(int[] array){
		for(int i = 0; i < array.length; i++){
			int num = array[i];
			TreeNode current = root;
			
			while(current != null){
				//Left or Right?
				if(num < current.data)
				{
					current = current.left;
				}
				else if (num > current.data)
				{
					current = current.right;
				}
				else
				{
					current.freq++;
				}
			}
			
			//We got current (means the place to put data)

			//current = new TreeNode(num);
			current.data = num;
			
		}
	}
	
	
}
