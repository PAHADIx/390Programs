
public class Queue {
	
	private int front, rear;
	private int[] queue = new int[100];
	
	public Queue()
	{
		front = 0;
		rear = -1;
	}
	
	public void push(int num)
	{
		if(rear >= 100){
			System.out.println("Queue Overflow");
		} else {
			rear++;
			queue[rear] = num;
				
		}
	}
	
	public int pop(){
		if(isEmpty()){
			System.out.println("Empty Queue");
			return -1;
		} else {
			int temp = queue[front];
			front++;
			return temp;	
		}
	}
	
	public boolean isEmpty(){
		if(front >= rear){
			return true;
		}
		return false;
	}
	
	

}
