public class LinkedList {
	public Node head;
	
	public void add(int num){
		if(head == null){
			head = new Node(num);			
		} else {
			Node temp = this.head;
			while(temp.next == null){
				temp.next = new Node(num);
			}
		}
	}
	
	public Node remove(){
		if(head == null){
			return null;
		} else {
			this.head = this.head.next;
			return this.head;
		}
	}
	
	public void print(){
		Node temp = this.head;
		if(temp == null){
			System.out.print("Empty List. ");
		} else {
			while(temp.next != null){
				System.out.print(temp.data + " ");
			}
		}
	}
	
	
	
	
}
