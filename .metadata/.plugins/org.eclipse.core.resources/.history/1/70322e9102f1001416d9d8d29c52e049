public class Tree {
	public TreeNode root = null;

	public Tree(){
		root = null;
	}
	
	public Tree(int[] array){
		for(int i = 0; i < array.length; i++){
			int num = array[i];
			TreeNode current = root;
			
			if(current != null){
				//Left or Right?
				if(num < current.data){
					current = current.left;
				}
				
				
				
				
			}
			
		
			
			
		}
	}
	
	
}
